<h2>Subscribers Table</h2>
	<table>
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Zip Code</th>
			</tr>
		</thead>
		<tbody>
			<% subscribers.forEach(subscriber => { %>
			<tr>
				<td>
					<a href="<%= `/subscribers/${subscriber._id}` %>">
						<%= subscriber.name %></td>
					</a>
				<td><%= subscriber.email %></td>
				<td><%= subscriber.zipCode %></td>
			</tr>
		<% }); %>
		</tbody>
	</table>