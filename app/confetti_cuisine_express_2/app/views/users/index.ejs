<h2>Users Table</h2>
	<table>
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Zip Code</th>
			</tr>
		</thead>
		<tbody>
			<% users.forEach(user => { %>
			<tr>
				<td>
					<a href="<%= `/users/${user._id}` %>">
						<%= user.fullName %></td>
					</a>
				<td><%= user.email %></td>
				<td><%= user.zipCode %></td>
				<td>
					<a href="<%= `/users/${user._id}/edit` %>">
						Edit
					</a>
				</td>
				<td>
					<a href="<%= `users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this recorde?')">Delete</a>
				</td>
			</tr>
		<% }); %>
		</tbody>
	</table>